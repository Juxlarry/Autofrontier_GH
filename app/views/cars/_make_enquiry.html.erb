  margin-left: 15px;
}

.field-holder{
  margin-bottom: 25px;
}


.atc-button {
  font-size: 1em;
  line-height: 1.125;
  border-radius: 6.25em;
  cursor: pointer;
  display: inline-block;
  margin: 0.5em 0;
  min-width: 216px;
  padding: 0.75em;
  text-align: center;
  text-decoration: none;
  transition: background 0.2s linear;
}

.lead--button-test .atc-button--primary {
    background-color: #0534ff;
    color: #fff;
}
.lead--button-test .atc-button {
    border: none;
    border-radius: 2px;
    cursor: pointer;
    display: inline-block;
    outline: none;
    padding: 8px 24px;
    text-align: center;
    text-decoration: none;
    transition: all .25s ease-in-out;
    font-size: 16px;
    line-height: 1.5;
    font-family: "Fabriga Regular",-apple-system,BlinkMacSystemFont,"Segoe UI",Helvetica,Arial,sans-serif;
}
.fpa__lead .leads-form__button {
    margin: 16px auto;
}
.fpa__lead .leads-form__button {
    width: 90%;
}

.fpa__lead .leads-form__actions-panel {
    background: #fff;
    bottom: 0;
    box-shadow: 0 -3px 4px -2px rgb(0 0 0 / 10%);
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding-bottom: 8px;
    position: -webkit-sticky;
    position: sticky;
    margin: 0 -16px;
    text-align: center;
}



.lead--button-test .atc-button--primary {
    background-color: #0534ff;
    color: #fff;
}

.lead--button-test .atc-button {
    border: none;
    border-radius: 2px;
    cursor: pointer;
    display: inline-block;
    outline: none;
    padding: 8px 24px;
    text-align: center;
    text-decoration: none;
    transition: all .25s ease-in-out;
