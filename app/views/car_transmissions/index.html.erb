<p id="notice"><%= notice %></p>

<h1>Car Transmissions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_transmissions.each do |car_transmission| %>
      <tr>
        <td><%= car_transmission.name %></td>
        <td><%= car_transmission.status %></td>
        <td><%= car_transmission.comments %></td>
        <td><%= link_to 'Show', car_transmission %></td>
        <td><%= link_to 'Edit', edit_car_transmission_path(car_transmission) %></td>
        <td><%= link_to 'Destroy', car_transmission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Transmission', new_car_transmission_path %>
