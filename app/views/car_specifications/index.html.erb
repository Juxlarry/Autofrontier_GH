<p id="notice"><%= notice %></p>

<h1>Car Specifications</h1>

<table>
  <thead>
    <tr>
      <th>Car</th>
      <th>Mileage</th>
      <th>Fuel</th>
      <th>Seats</th>
      <th>Doors</th>
      <th>Status</th>
      <th>Comments</th>
      <th>Description</th>
      <th>Gear type</th>
      <th>Body type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_specifications.each do |car_specification| %>
      <tr>
        <td><%= car_specification.car_id %></td>
        <td><%= car_specification.mileage %></td>
        <td><%= car_specification.fuel %></td>
        <td><%= car_specification.seats %></td>
        <td><%= car_specification.doors %></td>
        <td><%= car_specification.status %></td>
        <td><%= car_specification.comments %></td>
        <td><%= car_specification.description %></td>
        <td><%= car_specification.gear_type %></td>
        <td><%= car_specification.body_type %></td>
        <td><%= link_to 'Show', car_specification %></td>
        <td><%= link_to 'Edit', edit_car_specification_path(car_specification) %></td>
        <td><%= link_to 'Destroy', car_specification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Specification', new_car_specification_path %>
